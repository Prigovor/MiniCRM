<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="com.crm.database.entity.good.Good" id="goodLaptopHP">
        <property name="name" value="HP-011"/>
        <property name="description" value="HP-011 DESC"/>
        <property name="amount" value="6"/>
        <property name="price" value="1000"/>
    </bean>

    <bean class="com.crm.database.entity.good.Good" id="goodLaptopAcer">
        <property name="name" value="ACER-011"/>
        <property name="description" value="ACER-011 DESC"/>
        <property name="amount" value="4"/>
        <property name="price" value="1200"/>
    </bean>

    <bean class="com.crm.database.entity.good.Good" id="goodLaptopAsus">
        <property name="name" value="ASUS-011"/>
        <property name="description" value="ASUS-011 DESC"/>
        <property name="amount" value="12"/>
        <property name="price" value="1500"/>
    </bean>
</beans>